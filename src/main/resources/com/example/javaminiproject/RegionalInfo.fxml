<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="768.0" prefWidth="1366.0" stylesheets="@RegionalInfo.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javaminiproject.RegionalInfoController">
   <children>
      <Pane prefHeight="768.0" prefWidth="350.0" styleClass="navigation-pane">
         <children>
            <Pane prefHeight="114.0" prefWidth="119.0" styleClass="icon-pane" />
            <Pane layoutX="119.0" prefHeight="114.0" prefWidth="200.0" styleClass="title-pane">
               <children>
                  <Label layoutX="14.0" layoutY="41.0" text="Sea Explorer">
                     <font>
                        <Font name="Fira Code SemiBold" size="24.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <Pane layoutY="114.0" prefHeight="520.0" prefWidth="338.0">
               <children>
                  <Button layoutX="44.0" layoutY="27.0" mnemonicParsing="false" onAction="#onHomeButtonClicked" prefHeight="217.0" prefWidth="296.0" text="Home">
                     <font>
                        <Font name="Fira Code SemiBold" size="18.0" />
                     </font>
                     <graphic>
                        <ImageView id="home" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" styleClass="imageview">
                           <image>
                              <Image url="@home.png" />
                           </image>
                           <viewport>
                              <Rectangle2D />
                           </viewport>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button id="regional-info" layoutX="44.0" layoutY="97.0" mnemonicParsing="false" prefHeight="217.0" prefWidth="296.0" text="Regional Info">
                     <font>
                        <Font name="Fira Code SemiBold" size="18.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" styleClass="imageview">
                           <image>
                              <Image url="@location.png" />
                           </image>
                           <viewport>
                              <Rectangle2D />
                           </viewport>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button layoutX="44.0" layoutY="167.0" mnemonicParsing="false" prefHeight="217.0" prefWidth="296.0" text="Weather">
                     <font>
                        <Font name="Fira Code SemiBold" size="18.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" styleClass="imageview">
                           <image>
                              <Image url="@weather.png" />
                           </image>
                           <viewport>
                              <Rectangle2D />
                           </viewport>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button layoutX="44.0" layoutY="237.0" mnemonicParsing="false" prefHeight="217.0" prefWidth="296.0" text="Map">
                     <font>
                        <Font name="Fira Code SemiBold" size="18.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" styleClass="imageview">
                           <image>
                              <Image url="@map.png" />
                           </image>
                           <viewport>
                              <Rectangle2D />
                           </viewport>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </Pane>
         </children>
      </Pane>
      <Pane prefHeight="768.0" prefWidth="1060.0" styleClass="dashboard-pane" />
   </children>
</HBox>
